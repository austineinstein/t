{
  "name": "thou",
  "version": "1.0.0",
  "description": "Thou integrates the packages you need so you can write once, deploy everywhere. This tool supports our music/content model to sell products for our creators.",
  "main": "index.ts",
  "private": false,
  "scripts": {
    "dev": "bun --hot src/app/index.tsx",
    "build": "bun build src/app/index.tsx --outfile=dist/bundle.js",
    "start": "node bundle.js",
    "format": "bunx prettier --write .",
    "lint": "bunx eslint . --ext .ts,.tsx"
  },
  "author": "AUTHORS",
  "license": "ISC",
  "dependencies": {
    "@alchemy/aa-alchemy": "^3.19.0",
    "@alchemy/aa-core": "^3.19.0",
    "@helia/unixfs": "^4.0.3",
    "@jest/globals": "^29.7.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "bun": "^1.2.11",
    "dotenv": "^16.5.0",
    "helia": "^5.3.0",
    "multiformats": "^13.3.2",
    "oauth4webapi": "^3.5.0",
    "permissionless": "^0.2.44",
    "ts-node": "^10.9.2",
    "viem": "^2.28.3",
    "web3": "^4.16.0",
    "zksync-ethers": "^6.17.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.x"
  },
  "packageManager": "pnpm@8.3.1"
}
